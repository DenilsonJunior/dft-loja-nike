mixin tendenciesTree(textTop, cards)
    .tendenciesTreeTemplate
        .boxtitle
            p!=textTop
        .boxFlex
            each card in cards
                a.card(href=card.link, rel="noopener noreferrer")
                    .boxImg
                        img(src=card.img, alt=card.alt)
                    .boxText
                        h3!= card.title
